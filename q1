#include <stdio.h>
int main(){
    //if character is digit or not
    char ch;
    printf (" enter the character");
    scanf ("%c", &ch);

    if (ch >='0' && ch<='9')
     {
        printf ("%c is a digit", ch);
    }
    else 
    {
         printf (" %c is not a digit", ch);
    }
    return 0;
 
}
